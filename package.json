{
  "name": "latex-jp",
  "displayName": "LaTeX grammars JP",
  "description": "LaTeX textmate grammars for Visual Studio Code",
  "version": "1.16.0",
  "publisher": "jlelong",
  "license": "MIT",
  "homepage": "https://github.com/jlelong/vscode-latex-basics",
  "repository": {
    "type": "git",
    "url": "https://github.com/jlelong/vscode-latex-basics"
  },
  "author": {
    "name": "Jérôme Lelong"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "tex-jp",
        "extensions": [
          ".jp.sty",
          ".jp.cls",
          ".jp.bbx",
          ".jp.cbx"
        ],
        "configuration": "./languages/latex-language-configuration.json"
      },
      {
        "id": "doctex-jp",
        "extensions": [
          ".jp.dtx"
        ],
        "configuration": "./languages/doctex-language-configuration.json"
      },
      {
        "id": "latex-jp",
        "extensions": [
          ".jp.tex",
          ".jp.ltx",
          ".jp.ctx"
        ],
        "configuration": "./languages/latex-language-configuration.json"
      },
      {
        "id": "bibtex-jp",
        "extensions": [
          ".jp.bib"
        ],
        "configuration": "./languages/bibtex-language-configuration.json"
      },
      {
        "id": "bibtex-style-jp",
        "extensions": [
          ".jp.bst"
        ],
        "configuration": "./languages/bibtex-style-language-configuration.json"
      },
      {
        "id": "pweave-jp",
        "extensions": [
          ".jp.pnw",
          ".jp.ptexw"
        ],
        "configuration": "./languages/latex-language-configuration.json"
      },
      {
        "id": "jlweave-jp",
        "extensions": [
          ".jp.jnw",
          ".jp.jtexw"
        ],
        "configuration": "./languages/latex-language-configuration.json"
      },
      {
        "id": "rsweave-jp",
        "extensions": [
          ".jp.rnw",
          ".jp.Rnw",
          ".jp.Rtex",
          ".jp.rtex",
          ".jp.snw",
          ".jp.Snw"
        ],
        "configuration": "./languages/latex-language-configuration.json"
      },
      {
        "id": "cpp_embedded_latex-jp",
        "configuration": "./languages/latex-cpp-embedded-language-configuration.json",
        "aliases": []
      },
      {
        "id": "markdown_latex_combined-jp",
        "configuration": "./languages/markdown-latex-combined-language-configuration.json",
        "aliases": []
      }
    ],
    "grammars": [
      {
        "language": "tex-jp",
        "scopeName": "text.jp.tex",
        "path": "./syntaxes/TeX.tmLanguage.json",
        "unbalancedBracketScopes": [
          "keyword.control.ifnextchar.tex",
          "punctuation.math.operator.tex"
        ]
      },
      {
        "language": "doctex-jp",
        "scopeName": "text.jp.tex.doctex",
        "path": "./syntaxes/DocTeX.tmLanguage.json",
        "unbalancedBracketScopes": [
          "keyword.control.ifnextchar.tex",
          "punctuation.math.operator.tex"
        ]
      },
      {
        "language": "latex-jp",
        "scopeName": "text.jp.tex.latex",
        "path": "./syntaxes/LaTeX.tmLanguage.json",
        "unbalancedBracketScopes": [
          "keyword.control.ifnextchar.tex",
          "punctuation.math.operator.tex"
        ],
        "embeddedLanguages": {
          "source.asymptote": "asymptote",
          "source.cpp": "cpp_embedded_latex",
          "source.css": "css",
          "source.dot": "dot",
          "source.gnuplot": "gnuplot",
          "text.html": "html",
          "source.java": "java",
          "source.js": "javascript",
          "source.julia": "julia",
          "source.lua": "lua",
          "source.python": "python",
          "source.ruby": "ruby",
          "source.scala": "scala",
          "source.ts": "typescript",
          "text.xml": "xml",
          "source.yaml": "yaml",
          "meta.embedded.markdown_latex_combined": "markdown_latex_combined"
        }
      },
      {
        "language": "bibtex-jp",
        "scopeName": "text.jp.bibtex",
        "path": "./syntaxes/Bibtex.tmLanguage.json"
      },
      {
        "language": "bibtex-style-jp",
        "scopeName": "source.bst",
        "path": "./syntaxes/BibTeX-style.tmLanguage.json"
      },
      {
        "language": "markdown_latex_combined-jp",
        "scopeName": "text.jp.tex.markdown_latex_combined",
        "path": "./syntaxes/markdown-latex-combined.tmLanguage.json"
      },
      {
        "language": "cpp_embedded_latex-jp",
        "scopeName": "source.cpp.embedded.latex",
        "path": "./syntaxes/cpp-grammar-bailout.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.assembly.cpp": "asm"
        }
      },
      {
        "language": "pweave-jp",
        "scopeName": "text.jp.tex.latex.pweave",
        "path": "./syntax/Pweave.tmLanguage.json",
        "embeddedLanguages": {
          "source.python": "python"
        }
      },
      {
        "language": "jlweave",
        "scopeName": "text.tex.latex.jlweave",
        "path": "./syntax/JLweave.tmLanguage.json",
        "embeddedLanguages": {
          "source.julia": "julia"
        }
      },
      {
        "language": "rsweave",
        "scopeName": "text.tex.latex.rsweave",
        "path": "./syntax/RSweave.tmLanguage.json",
        "embeddedLanguages": {
          "source.r": "r"
        }
      }
    ]
  }
}
